    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "8080"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "justMyCode": true
        },
        {
            "type": "chrome",
            "name": "0.0.0.0:8080",
            "request": "launch",
            "url": "0.0.0.0:8080"
        }
    ]
}